echo Current exposure mode Auto 0, Shutter 2
uvcdynctrl --get "Exposure, Auto"
echo Setting exposure mode to Shutter 2
uvcdynctrl --set "Exposure, Auto" 2
echo New exposure mode Auto 0, Shutter 2
uvcdynctrl --get "Exposure, Auto"
uvcdynctrl --set "Exposure (Absolute)" 200
uvcdynctrl --set "Exposure (Absolute)" 1000
uvcdynctrl --get "Exposure (Absolute)"
sleep 1
echo New exposure mode Auto 0, Shutter 2
uvcdynctrl --set "Exposure, Auto" 0

